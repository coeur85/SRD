@model ICollection<KokiDB.Order>


@{
    ViewBag.PageName = "Orders filterd";
    Layout = "~/Views/Shared/_MasterLayout/_WebPages.cshtml";
}


@if (Model.Count > 0)
{

    foreach (var item in Model)
    {
        <div class="col-md-6">
            @Html.Partial("~/Views/Shared/_OrderThumbnail.cshtml", item)
        </div>
    }

}
else
{

    <div class="card">
        <div class="card-body">
            <h3>No data to show</h3>
        </div>
    </div>

}