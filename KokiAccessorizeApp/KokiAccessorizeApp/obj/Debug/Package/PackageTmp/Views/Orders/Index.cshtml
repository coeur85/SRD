@model ICollection<KokiDB.Order>

@{
    ViewBag.Title = "All Orders";
    Layout = "~/Views/Shared/_MasterLayout/_WebPages.cshtml";
    ViewBag.PageName = "All Statues";
}


@foreach (var item in Model)
{

    <div class="col-md-6">
        @Html.Partial("~/Views/Shared/_OrderThumbnail.cshtml", item)
    </div>


}
